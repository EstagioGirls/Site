 <header class="cabecalho" id="cabecalho">
    <div class="container">   
      <nav class="cabecalho-nav">
        <a id="cabecalho-nav-link" href="/contato">Contato</a>
        <img id="seta-direita" src = "./img/seta-direita.png" width="10" height="10"/>
        <a class="cabecalho-nav-link" id="Letra-home" href="/">Home</a>
      </nav>
    </div>
</header>
<main class= "pagina-contatos">
    <div class= "contatos">
        <p class="frase">Contatos</p>
    </div>
    <div class="conteudo-form">    
        <form class= "formulario" action="https://api.staticforms.xyz/submit" method="post">
            <input type="hidden" name="accessKey" value="53952fb0-9c57-4f25-a947-d9c9b0fb76e9">
            
            <div class="form">
                <label for="name">Nome:</label>
                <input name="name" id = "name" placeholder="Digite seu nome" type= "text">
            </div>
            <br>
            <div class="form">
                <label for="mail">E-mail:</label>
                <input name="email" id = "mail" placeholder="Digite seu E-mail" type= "text">
            </div>
            <br>
            <div class="form">
                <label for="assunto">Assunto:</label>
                <input name="assunto" id = "assunto"  placeholder="Digite o assunto" type= "text">
            </div>
            <br>
            <div class="form">
                <label for="msg">Mensagem:<br></label><br>
                <textarea name="message" id = "msg"  placeholder="Digite sua mensagem" cols="30" rows="5"></textarea>
            </div>
            <br>
            <div id="botao">
                <button type="submit" name="botao" value="ENVIAR MENSAGEM" class="botaoEnviar">ENVIAR</button>
            </div>  
            <input type="hidden" name="redirectTo" value="http://www.coltec.ufmg.br/coltec-ufmg/">
        </form>
    </div>
</main>
    